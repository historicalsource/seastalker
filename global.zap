	.FUNCT	PHONE-OFF
	ZERO?	REMOTE-PERSON /FALSE
	MOVE	REMOTE-PERSON,GLOBAL-OBJECTS
	SET	'REMOTE-PERSON,FALSE-VALUE
	SET	'REMOTE-PERSON-LOC,FALSE-VALUE
	SET	'REMOTE-PERSON-ON,FALSE-VALUE
	RETURN	REMOTE-PERSON-ON


