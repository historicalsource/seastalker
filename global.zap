	EQUAL?	HERE,SUB /?ELS111
	CALL	THIS-IS-IT,SUB
	PRINTI	"You have to be in the "
	PRINTD	SUB
	PRINTR	" to start it."
?ELS111:	FSET?	REACTOR,ONBIT /?ELS115
